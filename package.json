{
  "name": "pnp",
  "version": "2.0.0-alpha",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "check": "next lint && tsc --noEmit",
    "clean": "bunx rimraf node_modules .turbo dist out .eslintcache .next",
    "export": "next build && bun run sitemap && bun run search",
    "dev": "next dev --turbopack",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "bun install && bun run format:write && bun run devEnv next build && bunx serve@latest out",
    "start": "next start",
    "sitemap": "next-sitemap --config next-sitemap.config.cjs",
    "protest": "bun run export && bunx serve@latest out",
    "devEnv": "cross-env NEXT_PUBLIC_LOCALHOST_TEST=true NEXT_PUBLIC_LOG_ENABLE=true NODE_NO_WARNINGS=1",
    "protestDev": "bun run clean && bun install && bun run devEnv bun run protest",
    "search": "tsx src/app_function/scripts/build-algolia-search.ts",
    "typecheck": "tsc --noEmit",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache"
